<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.CompetitionInsurance">
	<typeAlias alias="CompetitionInsurance" type="cn.com.voge.gzvm.beans.CompetitionInsurance" />

	<resultMap id="CompetitionInsuranceMap" class="CompetitionInsurance">
		<result property="relaId" column="RELA_ID" />
		<result property="insuranceId" column="INSURANCE_ID" />
		<result property="competiId" column="COMPETI_ID" />

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_COMPETITION_INSURANCE
	</select>

	<select id="select" resultMap="CompetitionInsuranceMap">
		SELECT * FROM TB_COMPETITION_INSURANCE
		<dynamic prepend="WHERE">
			<isParameterPresent>
				RELA_ID=#value#
			</isParameterPresent>
		</dynamic>
		ORDER BY RELA_ID
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="CompetitionInsuranceMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="CompetitionInsuranceMap">
		SELECT * FROM TB_COMPETITION_INSURANCE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="relaId">
				RELA_ID=#relaId#
			</isNotNull>
			<isNotNull prepend="AND" property="insuranceId">
				INSURANCE_ID=#insuranceId#
			</isNotNull>
			<isNotNull prepend="AND" property="competiId">
				COMPETI_ID=#competiId#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
		ORDER BY RELA_ID
	</select>

	<insert id="insert" parameterClass="CompetitionInsurance">
		<selectKey resultClass="int" keyProperty="relaId">
			SELECT NEXTVAL('SQ_COMPETITION')
		</selectKey>
		INSERT INTO
		TB_COMPETITION_INSURANCE(RELA_ID,INSURANCE_ID,COMPETI_ID,
		VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#relaId#,#insuranceId#,#competiId#,
		#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>
	
	<delete id="delete" parameterClass="CompetitionInsurance">
		DELETE FROM TB_COMPETITION_INSURANCE WHERE RELA_ID=#relaId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_COMPETITION_INSURANCE WHERE RELA_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_COMPETITION_INSURANCE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="relaId">
				RELA_ID=#relaId#
			</isNotNull>
			<isNotNull prepend="AND" property="insuranceId">
				INSURANCE_ID=#insuranceId#
			</isNotNull>
			<isNotNull prepend="AND" property="competiId">
				COMPETI_ID=#competiId#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
	</delete>

	<update id="update" parameterClass="CompetitionInsurance">
		UPDATE TB_COMPETITION_INSURANCE SET INSURANCE_ID=#insuranceId#,COMPETI_ID=#competiId#,
			VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE RELA_ID=#relaId#
	</update>
	

</sqlMap>
