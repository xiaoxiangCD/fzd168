<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.UserPhoto">
	<typeAlias alias="UserPhoto" type="cn.com.voge.gzvm.beans.UserPhoto" />

	<resultMap id="UserPhotoMap" class="UserPhoto">
		<result property="photoId" column="PHOTO_ID" />
		<result property="userId" column="USER_ID" />
		<result property="photoName" column="PHOTO_NAME" />
		<result property="photoTime" column="PHOTO_TIME" />

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_USER_PHOTO
	</select>

	<select id="select" resultMap="UserPhotoMap">
		SELECT * FROM TB_USER_PHOTO
		<dynamic prepend="WHERE">
			<isParameterPresent>
				PHOTO_ID=#value#
			</isParameterPresent>
		</dynamic>
		ORDER BY PHOTO_ID
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="UserPhotoMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="UserPhotoMap">
		SELECT * FROM TB_USER_PHOTO
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="photoId">
				PHOTO_ID=#photoId#
			</isNotNull>
			<isNotNull prepend="AND" property="userId">
				USER_ID=#userId#
			</isNotNull>
			<isNotNull prepend="AND" property="photoName">
				PHOTO_NAME=#photoName#
			</isNotNull>
			<isNotNull prepend="AND" property="photoTime">
				PHOTO_TIME=#photoTime#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
		ORDER BY PHOTO_TIME
	</select>

	<insert id="insert" parameterClass="UserPhoto">
		<selectKey resultClass="int" keyProperty="photoId">
			SELECT NEXTVAL('SQ_USER')
		</selectKey>
		INSERT INTO
		TB_USER_PHOTO(PHOTO_ID,USER_ID,PHOTO_NAME,PHOTO_TIME,
		VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#photoId#,#userId#,#photoName#,#photoTime#,
		#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>
	
	<delete id="delete" parameterClass="UserPhoto">
		DELETE FROM TB_USER_PHOTO WHERE PHOTO_ID=#photoId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_USER_PHOTO WHERE PHOTO_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_USER_PHOTO
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="photoId">
				PHOTO_ID=#photoId#
			</isNotNull>
			<isNotNull prepend="AND" property="userId">
				USER_ID=#userId#
			</isNotNull>
			<isNotNull prepend="AND" property="photoName">
				PHOTO_NAME=#photoName#
			</isNotNull>
			<isNotNull prepend="AND" property="photoTime">
				PHOTO_TIME=#photoTime#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
	</delete>

	<update id="update" parameterClass="UserPhoto">
		UPDATE TB_USER_PHOTO SET
		PHOTO_ID=#photoId#,USER_ID=#userId#,PHOTO_NAME=#photoName#,PHOTO_TIME=#photoTime#,
		VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE PHOTO_ID=#photoId#
	</update>
	

</sqlMap>
