<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>voge taglibs</shortname>
	<uri>http://www.voge.com.cn/taglib</uri>
	
	<tag>
		<name>json</name>
		<tagclass>cn.com.voge.base.tag.BaseTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>object</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<name>include</name>
		<tagclass>cn.com.voge.base.tag.IncludeTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>file</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>encoding</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>

