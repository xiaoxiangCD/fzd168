<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.Discount">
	<typeAlias alias="Discount" type="cn.com.voge.gzvm.beans.Discount" />

	<resultMap id="DiscountMap" class="Discount">
		<result property="ruleId" column="RULE_ID" />
		<result property="competiId" column="COMPETI_ID" />
		<result property="passWord" column="PASS_WORD" />
		<result property="discount" column="DISCOUNT" />
		<result property="discountNum" column="DISCOUNT_NUM" />

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_DISCOUNT
	</select>

	<select id="select" resultMap="DiscountMap">
		SELECT * FROM TB_DISCOUNT
		<dynamic prepend="WHERE">
			<isParameterPresent>
				LAN_ID=#value#
			</isParameterPresent>
		</dynamic>
		ORDER BY RULE_ID
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="DiscountMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="DiscountMap">
		SELECT * FROM TB_DISCOUNT
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="ruleId">
				RULE_ID=#ruleId#
			</isNotNull>
			<isNotNull prepend="AND" property="competiId">
				COMPETI_ID=#competiId#
			</isNotNull>
			<isNotNull prepend="AND" property="passWord">
				PASS_WORD=#passWord#
			</isNotNull>
			<isNotNull prepend="AND" property="discount">
				DISCOUNT=#discount#
			</isNotNull>
			<isNotNull prepend="AND" property="discountNum">
				DISCOUNT_NUM=#discountNum#
			</isNotNull>
			<isNotNull prepend="AND" property="sendTime">
				SEND_TIME=#sendTime#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
		ORDER BY RULE_ID
	</select>

	<insert id="insert" parameterClass="Discount">
		<selectKey resultClass="int" keyProperty="ruleId">
			SELECT NEXTVAL('SQ_COMPETITION')
		</selectKey>
		INSERT INTO
		TB_DISCOUNT(RULE_ID,COMPETI_ID,PASS_WORD,DISCOUNT,DISCOUNT_NUM,
		VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#ruleId#,#competiId#,#passWord#,#discount#,#discountNum#,
		#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>
	
	<delete id="delete" parameterClass="Discount">
		DELETE FROM TB_DISCOUNT WHERE RULE_ID=#ruleId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_DISCOUNT WHERE RULE_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_DISCOUNT
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="ruleId">
				RULE_ID=#ruleId#
			</isNotNull>
			<isNotNull prepend="AND" property="competiId">
				COMPETI_ID=#competiId#
			</isNotNull>
			<isNotNull prepend="AND" property="passWord">
				PASS_WORD=#passWord#
			</isNotNull>
			<isNotNull prepend="AND" property="discount">
				DISCOUNT=#discount#
			</isNotNull>
			<isNotNull prepend="AND" property="discountNum">
				DISCOUNT_NUM=#discountNum#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
	</delete>

	<update id="update" parameterClass="Discount">
		UPDATE TB_DISCOUNT SET
		COMPETI_ID=#competiId#,PASS_WORD=#passWord#,DISCOUNT=#discount#,DISCOUNT_NUM=#discountNum#,
		VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE RULE_ID=#ruleId#
	</update>
	

</sqlMap>
