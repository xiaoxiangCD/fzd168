<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.OperModule">
	<typeAlias alias="OperModule" type="cn.com.voge.gzvm.beans.OperModule" />

	<resultMap id="OperModuleMap" class="OperModule">
		<result property="operId" column="OPER_ID" />
		<result property="moduleId" column="MODULE_ID" />
		<result property="roleId" column="ROLE_ID" />
		<result property="roleName" column="ROLE_NAME" />

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_OPER_MODULE
	</select>

	<select id="select" resultMap="AdminRelaMap">
		SELECT * FROM TB_OPER_MODULE
		<dynamic prepend="WHERE">
			<isParameterPresent>
				OPER_ID=#value#
			</isParameterPresent>
		</dynamic>
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="OperModuleMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="OperModuleMap">
		SELECT * FROM TB_OPER_MODULE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="operId">
				OPER_ID=#operId#
			</isNotNull>
			<isNotNull prepend="AND" property="moduleId">
				MODULE_ID=#moduleId#
			</isNotNull>
			<isNotNull prepend="AND" property="roleId">
				ROLE_ID=#roleId#
			</isNotNull>
			<isNotNull prepend="AND" property="roleName">
				ROLE_NAME=#roleName#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>
		</dynamic>
		ORDER BY OPER_ID
	</select>

	<insert id="insert" parameterClass="OperModule">
		<selectKey resultClass="int" keyProperty="operId">
			SELECT NEXTVAL('SQ_USER')
		</selectKey>
		INSERT INTO
		TB_OPER_MODULE(OPER_ID,MODULE_ID,ROLE_ID,ROLE_NAME,
		VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#operId#,#moduleId#,#roleId#,#roleName#,
		#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>

	<delete id="delete" parameterClass="OperModule">
		DELETE FROM TB_OPER_MODULE WHERE
		OPER_ID=#operId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_OPER_MODULE
		WHERE OPER_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_OPER_MODULE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="operId">
				OPER_ID=#operId#
			</isNotNull>
			<isNotNull prepend="AND" property="moduleId">
				MODULE_ID=#moduleId#
			</isNotNull>
			<isNotNull prepend="AND" property="roleId">
				ROLE_ID=#roleId#
			</isNotNull>
			<isNotNull prepend="AND" property="roleName">
				ROLE_NAME=#roleName#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>
		</dynamic>
	</delete>

	<update id="update" parameterClass="OperModule">
		UPDATE TB_OPER_MODULE SET
		OPER_ID=#operId#,MODULE_ID=#moduleId#,ROLE_ID=#roleId#,ROLE_NAME=#roleName#,
		VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE OPER_ID=#operId#
	</update>

</sqlMap>
