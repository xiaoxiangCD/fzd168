<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.Package">
	<typeAlias alias="Package" type="cn.com.voge.gzvm.beans.Package" />

	<resultMap id="PackageMap" class="Package">
		<result property="packageId" column="PACKAGE_ID" />
		<result property="packageName" column="PACKAGE_NAME" />
		<result property="cost" column="COST" />
		<result property="discountType" column="DISCOUNT_TYPE" />
		<result property="remark" column="REMARK" />

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_PACKAGE
	</select>

	<select id="select" resultMap="PackageMap">
		SELECT * FROM TB_PACKAGE
		<dynamic prepend="WHERE">
			<isParameterPresent>
				LAN_ID=#value#
			</isParameterPresent>
		</dynamic>
		ORDER BY PACKAGE_ID
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="PackageMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="PackageMap">
		SELECT * FROM TB_PACKAGE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="packageId">
				PACKAGE_ID=#packageId#
			</isNotNull>
			<isNotNull prepend="AND" property="packageName">
				PACKAGE_NAME=#packageName#
			</isNotNull>
			<isNotNull prepend="AND" property="cost">
				COST=#cost#
			</isNotNull>
			<isNotNull prepend="AND" property="discountType">
				DISCOUNT_TYPE=#discountType#
			</isNotNull>
			<isNotNull prepend="AND" property="remark">
				REMARK=#remark#
			</isNotNull>
			<isNotNull prepend="AND" property="sendTime">
				SEND_TIME=#sendTime#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
		ORDER BY PACKAGE_ID
	</select>

	<insert id="insert" parameterClass="Package">
		<selectKey resultClass="int" keyProperty="packageId">
			SELECT NEXTVAL('SQ_PACKAGE')
		</selectKey>
		INSERT INTO
		TB_PACKAGE(PACKAGE_ID,PACKAGE_NAME,COST,DISCOUNT_TYPE,REMARK,
		VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#packageId#,#packageName#,#cost#,#discountType#,#remark#,
		#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>
	
	<delete id="delete" parameterClass="Package">
		DELETE FROM TB_PACKAGE WHERE PACKAGE_ID=#packageId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_PACKAGE WHERE PACKAGE_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_PACKAGE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="packageId">
				PACKAGE_ID=#packageId#
			</isNotNull>
			<isNotNull prepend="AND" property="packageName">
				PACKAGE_NAME=#packageName#
			</isNotNull>
			<isNotNull prepend="AND" property="cost">
				COST=#cost#
			</isNotNull>
			<isNotNull prepend="AND" property="discountType">
				DISCOUNT_TYPE=#discountType#
			</isNotNull>
			<isNotNull prepend="AND" property="remark">
				REMARK=#remark#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
	</delete>

	<update id="update" parameterClass="Package">
		UPDATE TB_PACKAGE SET
		PACKAGE_NAME=#packageName#,COST=#cost#,DISCOUNT_TYPE=#discountType#,REMARK=#remark#,
		VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE PACKAGE_ID=#packageId#
	</update>
	

</sqlMap>
