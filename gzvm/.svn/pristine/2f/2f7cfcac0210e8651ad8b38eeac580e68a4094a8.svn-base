<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.OrderPackage">
	<typeAlias alias="OrderPackage" type="cn.com.voge.gzvm.beans.OrderPackage" />

	<resultMap id="OrderPackageMap" class="OrderPackage">
		<result property="relaId" column="RELA_ID" />
		<result property="orderId" column="ORDER_ID" />
		<result property="competiId" column="COMPETI_ID" />
		<result property="packageId" column="PACKAGE_ID" />
		<result property="count" column="COUNT" />
		<result property="number" column="NUMBER"/>

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_ORDER_PACKAGE
	</select>

	<select id="select" resultMap="OrderPackageMap">
		SELECT * FROM TB_ORDER_PACKAGE
		<dynamic prepend="WHERE">
			<isParameterPresent>
				RELA_ID=#value#
			</isParameterPresent>
		</dynamic>
		ORDER BY RELA_ID
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="OrderPackageMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="OrderPackageMap">
		SELECT * FROM TB_ORDER_PACKAGE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="relaId">
				RELA_ID=#relaId#
			</isNotNull>
			<isNotNull prepend="AND" property="userId">
				USER_ID=#userId#
			</isNotNull>
			<isNotNull prepend="AND" property="orderId">
				ORDER_ID=#orderId#
			</isNotNull>
			<isNotNull prepend="AND" property="competiId">
				COMPETI_ID=#competiId#
			</isNotNull>
			<isNotNull prepend="AND" property="packageId">
				PACKAGE_ID=#packageId#
			</isNotNull>
			<isNotNull prepend="AND" property="count">
				COUNT=#count#
			</isNotNull>
			<isNotNull prepend="AND" property="number">
				NUMBER=#number#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
		ORDER BY RELA_ID
	</select>

	<insert id="insert" parameterClass="OrderPackage">
		<selectKey resultClass="int" keyProperty="relaId">
			SELECT NEXTVAL('SQ_ORDER')
		</selectKey>
		INSERT INTO
		TB_ORDER_PACKAGE(RELA_ID,ORDER_ID,COMPETI_ID,PACKAGE_ID,COUNT,NUMBER,
		VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#relaId#,#orderId#,#competiId#,#packageId#,#count#,#number#,
		#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>
	
	<delete id="delete" parameterClass="OrderPackage">
		DELETE FROM TB_ORDER_PACKAGE WHERE RELA_ID=#relaId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_ORDER_PACKAGE WHERE RELA_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_ORDER_PACKAGE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="relaId">
				RELA_ID=#relaId#
			</isNotNull>
			<isNotNull prepend="AND" property="userId">
				USER_ID=#userId#
			</isNotNull>
			<isNotNull prepend="AND" property="orderId">
				ORDER_ID=#orderId#
			</isNotNull>
			<isNotNull prepend="AND" property="competiId">
				COMPETI_ID=#competiId#
			</isNotNull>
			<isNotNull prepend="AND" property="packageId">
				PACKAGE_ID=#packageId#
			</isNotNull>
			<isNotNull prepend="AND" property="count">
				COUNT=#count#
			</isNotNull>
			<isNotNull prepend="AND" property="number">
				NUMBER=#number#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
	</delete>

	<update id="update" parameterClass="OrderPackage">
		UPDATE TB_ORDER_PACKAGE SET
			ORDER_ID=#orderId#,COMPETI_ID=#competiId#,PACKAGE_ID=#packageId#,COUNT=#count#,NUMBER=#number#,
			VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE RELA_ID=#relaId#
	</update>
	

</sqlMap>
