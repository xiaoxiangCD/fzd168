body {
	line-height: 1;
}

:focus {
	outline: 1;
}

article, aside, canvas, details, figcaption, figure, footer, header,
	hgroup, menu, nav, section, summary {
	display: block;
}

ul {
	list-style: none;
}

li {
	list-style: none;
}

blockquote, q {
	quotes: none;
}

blockquote:before, blockquote:after, q:before, q:after {
	content: '';
	content: none;
}

a {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 100%;
	vertical-align: baseline;
	background: transparent;
}

a {
	text-decoration: none;
}

a:link {
	text-decoration: none; /* ָ���δ�����ʹ������*/
}

a:visited {
	text-decoration: none; /*ָ�Ѿ����ʹ������*/
}

a:hover {
	text-decoration: none; /*ָ���������*/
}

a:active {
	text-decoration: none; /* ָ���ڵ������*/
}

ins {
	background-color: #ff9;
	color: #000;
	text-decoration: none;
}

mark {
	background-color: #ff9;
	color: #000;
	font-style: italic;
	font-weight: bold;
}

del {
	text-decoration: line-through;
}

abbr[title], dfn[title] {
	border-bottom: 1px dotted #000;
	cursor: help;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

hr {
	display: block;
	height: 0.1em;
	border: 0;
	border-top: 1px solid #cccccc;
	margin: 1em 0;
	padding: 0;
}

input, select {
	vertical-align: middle;
}

input, input[type="button"], input[type="submit"], input[type="reset"],
	input[type="date"], input[type="moth"] { /* iPhone�?�ռ���ʽ��� */
	/* -webkit-appearance: none; */
	
}

textarea {
	-webkit-appearance: none;
	resize: none;
}

/*end*/

/* 头尾 */
#header {
	width: 100%;
}

#header .header_panel {
	background: #FFF;
	height: 6em;
	text-align: -webkit-center;
	overflow: hidden;
}

#header .header_item {
	width: 88%;
	max-width: 1200px;
}

#header .logo_panel {
	text-align: -webkit-left;
}

#header .user_panel {
	text-align: -webkit-right;
}

#header .logo_img {
	height: 5em;
	display: inline-block;
	vertical-align: top;
	margin-left: 1.6em;
}

#header .avalogo_img {
	height: auto;
	display: inline-block;
	margin-left: 1.6em;
	margin-top: 3em;
}

#header .head_img {
	height: 40px;
	width: 40px;
	display: inline-block;
	vertical-align: middle;
}

#header  .login_text {
	text-align: right;
	margin-right: 1.6em;
	color: #000000;
}

#header  .login_text a {
	color: #000000;
	margin: 0 10px;
	display: inline-block;
	margin-top: 3em;
}

#header .header_fonts {
	font-size: 1.8em;
	color: #FFF;
	height: 80px;
	line-height: 80px;
	display: inline-block;
}

#header .login_out {
	font-size: 1.2em;
	color: #FFF;
	height: 80px;
	line-height: 80px;
	display: inline-block;
}

#header .user_panel {
	display: inline-block;
}

#header .user_name {
	display: inline-block;
	font-size: 1.2em;
	color: #FFF;
}

/*footer��ʽ*/
#footer {
	width: 100%;
	/* 	margin-top:30px; */
}

#footer .footer_panel {
	background: url(/gzvm/client/images/bg.png) no-repeat top right;
	height: 60px;
	overflow: hidden;
	margin-left: auto;
	margin-right: auto;
	text-align: center;
	padding: 15px 0;
}

#footer .footer_item {
	color: #FFF;
	margin-top: 7px;
}

.footer_org, .footer_text {
	display: inline-block;
}

.footer_org {
	text-align: right;
}

.footer_text {
	text-align: left;
}

.bg {
	position: relative;
}

.bg_color {
	background-color: #000;
}

/*container*/
.container {
	padding: 0 !important;
	max-width: 1200px;
	margin-left: auto;
	margin-right: auto;
}

.content {
	padding-bottom: 5em;
}

/* 导航*/
.nav_panel {
	/* height:50px; */
	background: #ffd819;
}

.nav {
	max-width: 1200px;
	margin: 0 auto;
}

.nav li {
	vertical-align: middle;
	cursor: pointer;
}

.nav li a {
	display: inline-block;
	line-height: 50px;
	height: 100%;
	padding: 0 30px;
}

.nav li a span {
	float: left;
}

.nav_name {
	font-size: 1.5em;
	color: #000;
	display: inline-block;
	height: 100%;
	line-height: 50px;
	margin-left: 10px;
}

.nav_name_current {
	color: #ffd819 !important;
}

.index_icon {
	background: url(/gzvm/client/images/index.png) no-repeat center center;
	height: 100%;
	width: 24px;
	display: inline-block;
}

.index_icon_current {
	background: url(/gzvm/client/images/index_current.png) no-repeat center
		center;
	height: 100%;
	width: 24px;
	display: inline-block;
}

.competition_icon {
	background: url(/gzvm/client/images/competition.png) no-repeat center
		center;
	height: 100%;
	width: 24px;
	display: inline-block;
}

.competition_icon_current {
	background: url(/gzvm/client/images/competition_current.png) no-repeat
		center center;
	height: 100%;
	width: 24px;
	display: inline-block;
}

.about_icon {
	background: url(/gzvm/client/images/about.png) no-repeat center center;
	height: 100%;
	width: 24px;
	display: inline-block;
}

.about_icon_current {
	background: url(/gzvm/client/images/about_current.png) no-repeat center
		center;
	height: 100%;
	width: 24px;
	display: inline-block;
}

.current {
	background: #000;
}

.nav>li>a:hover, .nav>li>a:focus {
	background: none !important;
}

