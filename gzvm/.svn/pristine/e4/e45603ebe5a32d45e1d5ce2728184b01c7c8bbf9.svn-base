/*标题*/
.title {
	position: relative;
}

.title .title_icon, .title .title_cn, .title .title_en {
	display: inline-block;
	float: left;
}

.title .title_icon {
	margin-top: 8px;
}

.icon_img {
	min-width: 1.6em;
}

.title .title_cn {
	font-weight: bold;
	font-size: 1.5em;
	color: #000;
	padding: .5em .5em 0 0;
}

.title .title_en {
	font-weight: bold;
	font-size: 1.5em;
	color: #ffd819;
	padding: 5px 0;
}

.title .more {
	height: 24px;
	line-height: 24px;
	padding: 15px 0.6em;
}

}
.title .more a {
	color: #333;
}
/* 清楚浮动 */
.clear {
	clear: both;
}

/* 表单左边文字 */
.infor .col-md-1 {
	line-height: 34px;
	padding: 0px;
	margin-left: 0 10px 0 12px;
}

/* 温馨提示 */
.pop_panel {
	width: 720px;
	height: 400px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -360px;
	margin-top: -200px;
	background-color: #fff;
	border: solid 1px #ccc;
	box-shadow: 5px 3px 10px rgba(0, 0, 0, 0.3);
	display: none;
}

.pop_top {
	height: 40px;
	background-color: #666;
}

.pop_top p {
	margin-left: 20px;
	height: 40px;
	line-height: 40px;
	color: #fff;
}

.pop_exit {
	position: absolute;
	right: 10px;
	top: 14px;
	cursor: pointer;
}

.pop_exit img {
	width: 12px;
	height: 12px;
}

.pop_content {
	margin-top: 80px;
	padding: 0 100px;
}

.pop_hint {
	display: inline-block;
	float: left;
}

.pop_hint_l {
	width: 30%;
}

.pop_hint_r {
	width: 70%;
}

.hint_img {
	display: inline-block;
}

.pop_btn {
	width: 320px;
}

.success_hint {
	font-weight: bold;
	font-size: 20px;
}

