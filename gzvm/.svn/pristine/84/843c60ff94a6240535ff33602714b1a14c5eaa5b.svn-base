<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.CompetitionPackage">
	<typeAlias alias="CompetitionPackage" type="cn.com.voge.gzvm.beans.CompetitionPackage" />

	<resultMap id="CompetitionPackageMap" class="CompetitionPackage">
		<result property="productId" column="PRODUCT_ID" />
		<result property="packageId" column="PACKAGE_ID" />
		<result property="competiId" column="COMPETI_ID" />
		<result property="discountType" column="DISCOUNT_TYPE" />
		<result property="cost" column="COST" />

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_COMPETITION_PACKAGE
	</select>

	<select id="select" resultMap="CompetitionPackageMap">
		SELECT * FROM TB_COMPETITION_PACKAGE
		<dynamic prepend="WHERE">
			<isParameterPresent>
				LAN_ID=#value#
			</isParameterPresent>
		</dynamic>
		ORDER BY PRODUCT_ID
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="CompetitionPackageMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="CompetitionPackageMap">
		SELECT * FROM TB_COMPETITION_PACKAGE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="productId">
				PRODUCT_ID=#productId#
			</isNotNull>
			<isNotNull prepend="AND" property="packageId">
				PACKAGE_ID=#packageId#
			</isNotNull>
			<isNotNull prepend="AND" property="competiId">
				COMPETI_ID=#competiId#
			</isNotNull>
			<isNotNull prepend="AND" property="discountType">
				DISCOUNT_TYPE=#discountType#
			</isNotNull>
			<isNotNull prepend="AND" property="cost">
				COST=#cost#
			</isNotNull>
			<isNotNull prepend="AND" property="sendTime">
				SEND_TIME=#sendTime#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
		ORDER BY PRODUCT_ID
	</select>

	<insert id="insert" parameterClass="CompetitionPackage">
		<selectKey resultClass="int" keyProperty="productId">
			SELECT NEXTVAL('SQ_PACKAGE')
		</selectKey>
		INSERT INTO
		TB_COMPETITION_PACKAGE(PRODUCT_ID,PACKAGE_ID,COMPETI_ID,DISCOUNT_TYPE,COST,
			VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#productId#,#packageId#,#competiId#,#discountType#,#cost#,
			#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>
	
	<delete id="delete" parameterClass="CompetitionPackage">
		DELETE FROM TB_COMPETITION_PACKAGE WHERE PRODUCT_ID=#productId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_COMPETITION_PACKAGE WHERE PRODUCT_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_COMPETITION_PACKAGE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="productId">
				PRODUCT_ID=#productId#
			</isNotNull>
			<isNotNull prepend="AND" property="packageId">
				PACKAGE_ID=#packageId#
			</isNotNull>
			<isNotNull prepend="AND" property="competiId">
				COMPETI_ID=#competiId#
			</isNotNull>
			<isNotNull prepend="AND" property="discountType">
				DISCOUNT_TYPE=#discountType#
			</isNotNull>
			<isNotNull prepend="AND" property="cost">
				COST=#cost#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
	</delete>

	<update id="update" parameterClass="CompetitionPackage">
		UPDATE TB_COMPETITION_PACKAGE SET
			PACKAGE_ID=#packageId#,COMPETI_ID=#competiId#,DISCOUNT_TYPE=#discountType#,COST=#cost#,
			VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE PRODUCT_ID=#productId#
	</update>
	

</sqlMap>










