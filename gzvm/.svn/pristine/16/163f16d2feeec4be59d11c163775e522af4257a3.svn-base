


jQuery(document).ready(function($){
	getModule(function (result){
		console.log(result);
		var htmlStr = '';
		for(var i in result.results){
			var arr = result.results[i]
			htmlStr += '<div class="col-md-6 col-sm-6 col-xs-12 anli_mobile_list"><div class="anli_product_panel">'
					+'<a class="anli_product_name"><img src="/gzvm/fzd/images/show01.jpg" class="anli_product_img"></a>'
					+'<a class="anli_product_item">'+arr.contentTitle+'</a></div></div>';	
		}
		$('#contents').html(htmlStr);
		
		
	});
});