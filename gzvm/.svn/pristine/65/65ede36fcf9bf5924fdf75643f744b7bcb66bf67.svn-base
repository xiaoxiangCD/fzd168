<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.Module">
	<typeAlias alias="Module" type="cn.com.voge.gzvm.beans.Module" />

	<resultMap id="ModuleMap" class="Module">
		<result property="moduleId" column="MODULE_ID" />
		<result property="moduleName" column="MODULE_NAME" />
		<result property="moduleCode" column="MODULE_CODE" />
		<result property="description" column="DESCRIPTION" />
		<result property="dispIndex" column="DISP_INDEX" />

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_MODULE
	</select>

	<select id="select" resultMap="ModuleMap">
		SELECT * FROM TB_MODULE
		<dynamic prepend="WHERE">
			<isParameterPresent>
				MODULE_ID=#value#
			</isParameterPresent>
		</dynamic>
		ORDER BY MODULE_ID
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="ModuleMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="ModuleMap">
		SELECT * FROM TB_MODULE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="moduleId">
				MODULE_ID=#moduleId#
			</isNotNull>
			<isNotNull prepend="AND" property="moduleName">
				MODULE_NAME=#moduleName#
			</isNotNull>
			<isNotNull prepend="AND" property="moduleCode">
				MODULE_CODE=#moduleCode#
			</isNotNull>
			<isNotNull prepend="AND" property="description">
				DESCRIPTION=#description#
			</isNotNull>
			<isNotNull prepend="AND" property="dispIndex">
				DISP_INDEX=#dispIndex#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
		ORDER BY MODULE_ID
	</select>

	<insert id="insert" parameterClass="Module">
		<selectKey resultClass="int" keyProperty="moduleId">
			SELECT NEXTVAL('SQ_MODULE')
		</selectKey>
		INSERT INTO
		TB_MODULE(MODULE_ID,MODULE_NAME,MODULE_CODE,DESCRIPTION,DISP_INDEX,
		VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#moduleId#,#moduleName#,#moduleCode#,#description#,#dispIndex#,
		#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>
	
	<delete id="delete" parameterClass="Module">
		DELETE FROM TB_MODULE WHERE MODULE_ID=#moduleId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_MODULE WHERE MODULE_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_MODULE
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="moduleId">
				MODULE_ID=#moduleId#
			</isNotNull>
			<isNotNull prepend="AND" property="moduleName">
				MODULE_NAME=#moduleName#
			</isNotNull>
			<isNotNull prepend="AND" property="moduleCode">
				MODULE_CODE=#moduleCode#
			</isNotNull>
			<isNotNull prepend="AND" property="description">
				DESCRIPTION=#description#
			</isNotNull>
			<isNotNull prepend="AND" property="dispIndex">
				DISP_INDEX=#dispIndex#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>

		</dynamic>
	</delete>

	<update id="update" parameterClass="Module">
		UPDATE TB_MODULE SET
			MODULE_NAME=#moduleName#,MODULE_CODE=#moduleCode#,DESCRIPTION=#description#,DISP_INDEX=#dispIndex#,
			VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE MODULE_ID=#moduleId#
	</update>
	

</sqlMap>
