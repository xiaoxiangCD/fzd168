<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="cn.com.voge.gzvm.beans.AdminRela">
	<typeAlias alias="AdminRela" type="cn.com.voge.gzvm.beans.AdminRela" />

	<resultMap id="AdminRelaMap" class="AdminRela">
		<result property="relaId" column="RELA_ID" />
		<result property="adminId" column="ADMIN_ID" />
		<result property="roleId" column="ROLE_ID" />

		<result property="versionNum" column="VER_NUM" nullValue="0" />
		<result property="instanceId" column="INSTANCE_ID" nullValue="0" />
		<result property="state" column="STATE" nullValue="0" />
		<result property="createUser" column="CREATE_USER" nullValue="0" />
		<result property="createDate" column="CREATE_DATE" javaType="date" />
	</resultMap>

	<select id="count" resultClass="int">
		SELECT count(*) FROM TB_ADMIN_RELA
	</select>

	<select id="select" resultMap="AdminRelaMap">
		SELECT * FROM TB_ADMIN_RELA
		<dynamic prepend="WHERE">
			<isParameterPresent>
				RELA_ID=#value#
			</isParameterPresent>
		</dynamic>
	</select>

	<select id="selectBySql" parameterClass="string" resultMap="AdminRelaMap">
		$value$
	</select>

	<select id="selectByMap" parameterClass="map" resultMap="AdminRelaMap">
		SELECT * FROM TB_ADMIN_RELA
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="relaId">
				RELA_ID=#relaId#
			</isNotNull>
			<isNotNull prepend="AND" property="adminId">
				ADMIN_ID=#adminId#
			</isNotNull>
			<isNotNull prepend="AND" property="token">
				ROLE_ID=#roleId#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>
		</dynamic>
		ORDER BY RELA_ID
	</select>

	<insert id="insert" parameterClass="AdminRela">
		<selectKey resultClass="int" keyProperty="relaId">
			SELECT NEXTVAL('SQ_USER')
		</selectKey>
		INSERT INTO
		TB_ADMIN_RELA(RELA_ID,ADMIN_ID,ROLE_ID,
		VER_NUM,STATE,CREATE_USER,CREATE_DATE,INSTANCE_ID)
		VALUES(#relaId#,#adminId#,#roleId#,
		#versionNum#,#state#,#createUser#,#createDate#,#instanceId#)
	</insert>

	<delete id="delete" parameterClass="AdminRela">
		DELETE FROM TB_ADMIN_RELA WHERE
		RELA_ID=#relaId#
	</delete>

	<delete id="deleteByPK" parameterClass="int">
		DELETE FROM TB_ADMIN_RELA
		WHERE RELA_ID=#value#
	</delete>

	<delete id="deleteByMap" parameterClass="map">
		DELETE FROM TB_ADMIN_RELA
		<dynamic prepend="WHERE">
			<isNotNull prepend="AND" property="relaId">
				RELA_ID=#relaId#
			</isNotNull>
			<isNotNull prepend="AND" property="adminId">
				ADMIN_ID=#adminId#
			</isNotNull>
			<isNotNull prepend="AND" property="token">
				ROLE_ID=#roleId#
			</isNotNull>
			<isNotNull prepend="AND" property="state">
				(STATE &amp; #state#) >0
			</isNotNull>
			<isNotNull prepend="AND" property="authoritySQL">
				$authoritySQL$
			</isNotNull>
		</dynamic>
	</delete>

	<update id="update" parameterClass="AdminRela">
		UPDATE TB_ADMIN_RELA SET
		RELA_ID=#relaId#,ADMIN_ID=#adminId#,ROLE_ID=#roleId#,
		VER_NUM=#versionNum#,STATE=#state#,CREATE_USER=#createUser#,CREATE_DATE=#createDate#,INSTANCE_ID=#instanceId#
		WHERE RELA_ID=#relaId#
	</update>

</sqlMap>
