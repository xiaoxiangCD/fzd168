<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>登录</title>
<link rel="stylesheet" href="../../css/bootstrap.css">
<link rel="stylesheet" href="../../css/common.css">
<link rel="stylesheet" href="../../css/bootstrap-table.css">
<link rel="stylesheet" href="../css/login.css">
<script type="text/javascript" src="../../js/jquery-1.11.3.js"></script>
<script type="text/javascript" src="../../js/bootstrap.js"></script>
<script type="text/javascript" src="../../js/bootstrap-table.js"></script>
<script type="text/javascript" src="../js/login.js"></script>
</head>
<body>
	<div id="header">
		<div class="header_panel" >
			<div class="header_item">
				<div class="row">
					<div class="col-md-3 logo_panel">
						<img alt="" src="../images/chuima.png" class="img-responsive logo_img">
					</div>
					<div class="col-md-9 logo_panel">
						<p class="header_fonts">| 欢迎登录</p>
					</div>
				</div>
			</div>
		</div>
	</div>
	
	<div class="container" id="container">
		<div id="section" class="row login_panel">
			<div class="col-md-6"></div>
			<div class="col-md-6 edit_panel">
				<div class="login_space">
					<div class="login_row">
						<p style="font-size:20px;">管理员登录</p>
					</div>
					<div class="login_row">
						<input class="form-control vg_input_text" type="text" name="user" placeholder="账号">
					</div>
					<div class="login_row">
						<input class="form-control vg_input_text"  type="password" name="pwd" placeholder="密码">
					</div>
					<div class="login_row">
						<button class="btn btn-block btn-warning vg_btn">登录</button>
					</div>
				</div>
			</div>
		</div>
	</div>
	
	<div id="footer">
		<div class="footer_panel" >
			<p class="footer_item">广州垂马体育发展有限公司</p>
		</div>
	</div>
</body>
</html>